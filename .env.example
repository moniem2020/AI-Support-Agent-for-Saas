# Google Gemini API Keys
# Main API key (required)
GOOGLE_API_KEY=your_main_api_key_here

# Fast API key for simple queries (optional, falls back to GOOGLE_API_KEY)
GOOGLE_API_KEY_FAST=your_fast_api_key_here

# API Key Pool for rotation when quota is exceeded (comma-separated, recommended: 4 keys)
# The system will automatically rotate through these keys when one hits quota limit
GOOGLE_API_KEYS_POOL=key1,key2,key3,key4

# Model Configuration
GEMINI_MODEL=gemini-2.5-flash
EMBEDDING_MODEL=models/text-embedding-004

# Cache Settings
SEMANTIC_CACHE_THRESHOLD=0.90
CACHE_TTL_SECONDS=3600

# Retrieval Settings
DENSE_TOP_K=10
SPARSE_TOP_K=10
RERANK_TOP_K=5

# Agent Settings
MAX_RETRIES=2
CONFIDENCE_THRESHOLD=0.7
ESCALATION_THRESHOLD=0.5

# API Settings (used by Flask UI)
API_BASE_URL=http://localhost:8000/api/v1
